    "body": "OUTPUT_DSST_MEAN_ELEMENT_RATES_ZONAL_TERMS  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the mean element rates due to zonal terms of the spherical harmonic expansion of the Earth gravity field."

  "Output mean element rates due to Sun gravity":
    "prefix": "OUTPUT_DSST_MEAN_ELEMENT_RATES_SUN_GRAVITY"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_MEAN_ELEMENT_RATES_SUN_GRAVITY  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the mean element rates due to the Sun's third-body central gravity."

  "Output mean element rates due to Moon gravity":
    "prefix": "OUTPUT_DSST_MEAN_ELEMENT_RATES_MOON_GRAVITY"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_MEAN_ELEMENT_RATES_MOON_GRAVITY  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the mean element rates due to the Moon's third-body central gravity."

  "Output mean element rates due to atmospheric drag":
    "prefix": "OUTPUT_DSST_MEAN_ELEMENT_RATES_ATMOSPHERIC_DRAG"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_MEAN_ELEMENT_RATES_ATMOSPHERIC_DRAG  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the mean element rates due to the Earth's atmospheric drag."

  "Output mean element rates due to solar radiation pressure":
    "prefix": "OUTPUT_DSST_MEAN_ELEMENT_RATES_SOLAR_RADIATION_PRESSURE"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_MEAN_ELEMENT_RATES_SOLAR_RADIATION_PRESSURE  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the mean element rates due to solar radiation pressure."

  "Output total short period terms":
    "prefix": "OUTPUT_DSST_SHORT_PERIOD_TERMS"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_SHORT_PERIOD_TERMS  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the short period terms due to all the included disturbing forces."

  "Output short period terms due to zonal terms":
    "prefix": "OUTPUT_DSST_SHORT_PERIOD_TERMS_ZONAL_TERMS"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_SHORT_PERIOD_TERMS_ZONAL_TERMS  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the short period terms due to zonal terms of the spherical harmonic expansion of the Earth gravity field."

  "Output short period terms due to Sun gravity":
    "prefix": "OUTPUT_DSST_SHORT_PERIOD_TERMS_SUN_GRAVITY"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_SHORT_PERIOD_TERMS_SUN_GRAVITY  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the short period terms due to the Sun's third-body central gravity."

  "Output short period terms due to Moon gravity":
    "prefix": "OUTPUT_DSST_SHORT_PERIOD_TERMS_MOON_GRAVITY"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_SHORT_PERIOD_TERMS_MOON_GRAVITY  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the short period terms due to the Moon's third-body central gravity."

  "Output short period terms due to atmospheric drag":
    "prefix": "OUTPUT_DSST_SHORT_PERIOD_TERMS_ATMOSPHERIC_DRAG"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_SHORT_PERIOD_TERMS_ATMOSPHERIC_DRAG  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the short period terms due to the Earth's atmospheric drag."

  "Output short period terms due to solar radiation pressure":
    "prefix": "OUTPUT_DSST_SHORT_PERIOD_TERMS_SOLAR_RADIATION_PRESSURE"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_SHORT_PERIOD_TERMS_SOLAR_RADIATION_PRESSURE  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the short period terms due to solar radiation pressure."

  "Output total computation times":
    "prefix": "OUTPUT_DSST_COMPUTATION_TIMES"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_COMPUTATION_TIMES  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the total computation times for all the included disturbing forces."

  "Output computation times for zonal terms":
    "prefix": "OUTPUT_DSST_COMPUTATION_TIMES_ZONAL_TERMS"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_COMPUTATION_TIMES_ZONAL_TERMS  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the computation times for zonal terms of the spherical harmonic expansion of the Earth gravity field."

  "Output computation times for Sun gravity":
    "prefix": "OUTPUT_DSST_COMPUTATION_TIMES_SUN_GRAVITY"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_COMPUTATION_TIMES_SUN_GRAVITY  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the computation times for the Sun's third-body central gravity."

  "Output computation times for Moon gravity":
    "prefix": "OUTPUT_DSST_COMPUTATION_TIMES_MOON_GRAVITY"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_COMPUTATION_TIMES_MOON_GRAVITY  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the computation times for the Moon's third-body central gravity."

  "Output computation times for atmospheric drag":
    "prefix": "OUTPUT_DSST_COMPUTATION_TIMES_ATMOSPHERIC_DRAG"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_COMPUTATION_TIMES_ATMOSPHERIC_DRAG  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the computation times for the Earth's atmospheric drag."

  "Output computation times for solar radiation pressure":
    "prefix": "OUTPUT_DSST_COMPUTATION_TIMES_SOLAR_RADIATION_PRESSURE"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_COMPUTATION_TIMES_SOLAR_RADIATION_PRESSURE  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the computation times for solar radiation pressure."

  "Output frequency":
    "prefix": "OUTPUT_ONE_IN_EVERY_INTEGRATION_STEPS"
    "leftLabel": "int"
    "body": "OUTPUT_ONE_IN_EVERY_INTEGRATION_STEPS  ${1:1}${2:  % [1, INF)${3:\n}}"
    "description": "Output the requested variables only at one in every OUTPUT_ONE_IN_EVERY_INTEGRATION_STEPS steps. If set to 1, all the integration steps are outputted. This variable is ignored when OUTPUT_ONLY_LAST_INTEGRATION_STEP is set to YES."

  "Output only last integration step":
    "prefix": "OUTPUT_ONLY_LAST_INTEGRATION_STEP"
    "leftLabel": "bool"
    "body": "OUTPUT_ONLY_LAST_INTEGRATION_STEP  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the results matrix should only contain the values for the requested variables at the last integration step. If set to YES, OUTPUT_ONE_IN_EVERY_INTEGRATION_STEPS is ignored."

  "Print success message":
    "prefix": "PRINT_SUCCESS_MESSAGE_AFTER_PROPAGATION"
    "leftLabel": "bool"
    "body": "PRINT_SUCCESS_MESSAGE_AFTER_PROPAGATION  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether a message should be printed to the console or terminal window when the propagation completes successfully, including the path of the original input file."

  "Delete input file":
    "prefix": "DELETE_INPUT_FILE_AFTER_PROPAGATION"
    "leftLabel": "bool"
    "body": "DELETE_INPUT_FILE_AFTER_PROPAGATION  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the original input file should be deleted after successfully completing the propagation. This does not delete other input files referenced from the original input file using #INCLUDE statements."

  "Skip propagation if output exists":
    "prefix": "SKIP_PROPAGATION_IF_OUTPUT_FILE_EXISTS"
    "leftLabel": "bool"
    "body": "SKIP_PROPAGATION_IF_OUTPUT_FILE_EXISTS  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to skip the propagation if the output file specified by OUTPUT_DIRECTORY_PATH and OUTPUT_FILE_NAME exists. Note that, when set to YES, if a propagation is completed and then the input file is changed, it will not be possible to propagate the orbit with the updated input settings unless the previously-generated output file is (re)moved manually by the user."

  "Skip propagation if results found":
    "prefix": "SKIP_PROPAGATION_IF_RESULTS_FOUND"
    "leftLabel": "bool"
    "body": "SKIP_PROPAGATION_IF_RESULTS_FOUND  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to skip the propagation if an output file containing the requested results for the current input settings is found (the output files in which the results are searched for are determined by SEARCH_OUTPUT_DIRECTORY_PATH and SEARCH_OUTPUT_FILENAME_PATTERN; only the output files generated with OUTPUT_INPUT_SETTINGS set to YES are considered, otherwise it is not possible to determine if the input settings for the candidate output file and the current input settings match)."

  "Allow resuming propagation from results":
    "prefix": "ALLOW_RESUMING_PROPAGATION_FROM_RESULTS"
    "leftLabel": "bool"
    "body": "ALLOW_RESUMING_PROPAGATION_FROM_RESULTS  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether it should be allowed to propagate an orbit using the results from a previously-generated output file (the output files in which the results are searched for are determined by SEARCH_OUTPUT_DIRECTORY_PATH and SEARCH_OUTPUT_FILENAME_PATTERN; only the output files generated with OUTPUT_INPUT_SETTINGS set to YES are considered, otherwise it is not possible to determine if the input settings for the candidate output file and the current input settings match). When set to YES, if a candidate output file contains the body state for the last integration step and the most restrictive of END_EPOCH and PROPAGATION_PERIOD is larger for the current propagation than for the previously-propagated orbit, then the propagation is resumed from that final epoch and state."

  "Search for output in directory":
    "prefix": "SEARCH_OUTPUT_DIRECTORY_PATH"
    "leftLabel": "std::string"
    "body": "SEARCH_OUTPUT_DIRECTORY_PATH  '$1'${2:\n}"
    "description": "The (relative or absolute) path of the directory in which it will be searched for output files containing the results corresponding to the current propagation."

  "Search for output in files named":
    "prefix": "SEARCH_OUTPUT_FILENAME_PATTERN"
    "leftLabel": "std::string"
    "body": "SEARCH_OUTPUT_FILENAME_PATTERN  '${1:.+}'${3:\n}"
    "description": "The regular expression used to determine in which files it will be searched for the results corresponding to the current propagation. The program will only search in \"tespout\" files whose name matches the provided expression."

  "Search in subdirectories":
    "prefix": "SEARCH_IN_SUBDIRECTORIES"
    "leftLabel": "bool"
    "body": "SEARCH_IN_SUBDIRECTORIES  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the search for matching files should be performed recursively in all the subdirectories contained in SEARCH_OUTPUT_DIRECTORY_PATH."

  "Search ignore file contents":
    "prefix": "SEARCH_IGNORE_FILE_CONTENTS"
    "leftLabel": "bool"
    "body": "SEARCH_IGNORE_FILE_CONTENTS  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the search for matching results is considered to be successful when a file matching the specified SEARCH_OUTPUT_FILENAME_PATTERN in the SEARCH_OUTPUT_DIRECTORY_PATH (and subdirectories) is found, regardless of the input settings that were used to generate that file. When set to NO, the match is only successful when all the relevant input settings of the candidate file match those of the current propagation."

  "Output for skipped propagation":
    "prefix": "OUTPUT_FOR_SKIPPED_PROPAGATION"
    "leftLabel": "enum"
    "body": "OUTPUT_FOR_SKIPPED_PROPAGATION  ${1:DUPLICATE}${2:  % NONE | REFERENCE | DUPLICATE${3:\n}}"
    "description": "The contents of the file (if any) that will be generated when an orbit is skipped because an output file form a previous propagation containing the requested results for the current input settings has been found. Possible values: NONE, no output file is generated; REFERENCE, an output file containing the path to the matching file is generated; DUPLICATE, a file containing the requested results, obtained by copying them from the matching file, is generated. When the path of the output file and the matching file are the same, this variable is ignored and the matching file stays unmodified."

  "Non-defined variable":
    "prefix": "NON_DEFINED_VARIABLE"
    "leftLabel": "enum"
    "body": "NON_DEFINED_VARIABLE  ${1:USE_DEFAULT}${2:  % USE_DEFAULT | WARN | ERROR${3:\n}}"
    "description": "The response to a non-defined variable event. Possible values: USE_DEFAULT, the default value is used if the variable is not defined in the input file; WARN, the same, but a message is printed to the console or terminal window; ERROR, the program terminates throwing an error message."

  "Unrecognized variable name":
    "prefix": "UNRECOGNIZED_VARIABLE_NAME"
    "leftLabel": "enum"
    "body": "UNRECOGNIZED_VARIABLE_NAME  ${1:WARN}${2:  % IGNORE | WARN | ERROR${3:\n}}"
    "description": "The response to an unrecognized variable name event. Possible values: IGNORE, the variable and specified value are ignored; WARN, the same, but a message is printed to the console or terminal window; ERROR, the program terminates throwing an error message."

  "Multiply-defined variable in different input files":
    "prefix": "MULTIPLY_DEFINED_VARIABLE_DIF_FILES"
    "leftLabel": "enum"
    "body": "MULTIPLY_DEFINED_VARIABLE_DIF_FILES  ${1:USE_LAST}${2:  % USE_LAST | WARN | ERROR${3:\n}}"
    "description": "The response to a multiply-defined variable in different input files event (i.e. in included files). Possible values: USE_LAST, the value of the last definition is used; WARN, the same, but a message is printed to the console or terminal window; ERROR, the program terminates throwing an error message."

  "Multiply-defined variable in the same input file":
    "prefix": "MULTIPLY_DEFINED_VARIABLE_SAME_FILE"
    "leftLabel": "enum"
    "body": "MULTIPLY_DEFINED_VARIABLE_SAME_FILE  ${1:WARN}${2:  % USE_LAST | WARN | ERROR${3:\n}}"
    "description": "The response to a multiply-defined variable in the same input file event. Possible values: USE_LAST, the value of the last definition is used; WARN, the same, but a message is printed to the console or terminal window; ERROR, the program terminates throwing an error message."

  "Non-valid value":
    "prefix": "NON_VALID_VALUE"
    "leftLabel": "enum"
    "body": "NON_VALID_VALUE  ${1:ERROR}${2:  % USE_DEFAULT | WARN | ERROR${3:\n}}"
    "description": "The response to a non-valid value event. Possible values: USE_DEFAULT, the default value is used if the provided value is outside the allowed range or does not match any of the possible values; WARN, the same, but a message is printed to the console or terminal window; ERROR, the program terminates throwing an error message."

  "Skipping propagation":
    "prefix": "SKIPPING_PROPAGATION"
    "leftLabel": "enum"
    "body": "SKIPPING_PROPAGATION  ${1:WARN}${2:  % SILENT | WARN${3:\n}}"
    "description": "The response to a skipping propagationevent. Possible values: SILENT, no message is printed; WARN, a message is printed to the console or terminal window."

  "Resuming propagation from output":
    "prefix": "RESUMING_PROPAGATION_FROM_OUTPUT"
    "leftLabel": "enum"
    "body": "RESUMING_PROPAGATION_FROM_OUTPUT  ${1:WARN}${2:  % SILENT | WARN${3:\n}}"
    "description": "The response to a resuming propagation from output event. Possible values: SILENT, no message is printed; WARN, a message is printed to the console or terminal window."


# tespin snippets
".source.tespin":

  "Include statement":
    "prefix": "include"
    "body": "#INCLUDE '$1'"
    "description": "Include other input files to be parsed before the contents of this file. Relative or absolute paths can be used. Relative paths are defined with respect to the directory in which the file containing the include statement is located."

  "Lists all supported parameters with their default values":
    "prefix": "list"
    "body": '''
      %
      % List of all the parameters that can be defined in "tespin" files.
      % The default values are provided, together with a comment indicating the possible (range of) values.
      %
      % TESP - Tudat Earth Satellite Propagator - http://github.com/aleixpinardell/tesp/
      % The installed language package is compatible with TESP 0.1.0 or earlier.
      % Copyright (c) 2017 TU Delft.
      %
      $1
      % #INCLUDE 'path to an input file to be parsed before this one'


      % ACCELERATION MODEL

      GEOPOTENTIAL_MODEL  GGM02C  % GGM02C | GGM02S
      GEOPOTENTIAL_DEGREE  2  % [1, 160]
      GEOPOTENTIAL_ORDER  0  % [0, 160]

      THIRD_BODY_ATTRACTION_SUN  YES  % NO | YES
      THIRD_BODY_ATTRACTION_MOON  YES  % NO | YES

      ATMOSPHERIC_DRAG  YES  % NO | YES
      ATMOSPHERIC_MODEL  NRLMSISE00  % NRLMSISE00 | EXPONENTIAL
      SPACE_WEATHER_FILE_PATH  ''

      SOLAR_RADIATION_PRESSURE  YES  % NO | YES
      IGNORE_ECLIPSES  NO  % NO | YES


      % BODY PROPERTIES

      BODY_MASS  100.0  % (0, INF) kg
      BODY_CROSS_SECTIONAL_AREA  10.0  % (0, INF) m^2

      ALTITUDE_DEPENDENT_DRAG_COEFFICIENT  NO  % NO | YES
      BODY_DRAG_COEFFICIENT  2.5  % (0, INF)
      BODY_RADIATION_PRESSURE_COEFFICIENT  1.5  % [1, 2]


      % INITIAL STATE

      INITIAL_EPOCH  0.0  % (-INF, INF) seconds since J2000, or formatted date 'YYYY-MM-DD hh:mm:ss'

      INITIAL_SEMIMAJOR_AXIS  7371.0  % (6371, INF) km
      INITIAL_ECCENTRICITY  0.0  % [0, 1)
      INITIAL_INCLINATION  0.0  % [0, 180] deg
      INITIAL_ARGUMENT_PERIGEE  0.0  % [0, 360] deg
      INITIAL_LONGITUDE_ASCENDING_NODE  0.0  % [0, 360] deg
      INITIAL_TRUE_ANOMALY  0.0  % [0, 360] deg

      % INITIAL_PERIGEE_ALTITUDE  1000.0  % (0, INF) km
      % INITIAL_APOGEE_ALTITUDE  1000.0  % (0, INF) km


      % PROPAGATION SETTINGS

      PRELOAD_CELESTIAL_BODIES_DATA  YES  % NO | YES

      % The most restrective of these three will determine the actual duration of the propagation:
      END_EPOCH  '2015-12-31'  % (-INF, INF) second since J2000, or formatted date 'YYYY-MM-DD hh:mm:ss'
      PROPAGATION_PERIOD  3.1558e8  % (0, INF) seconds
      REENTRY_ALTITUDE  0.0  % [0, INF) km

      PROPAGATOR_NAME  COWELL  % COWELL | ENCKE

      INTEGRATOR_NAME  RK4  % RK4 | RK45 | RK56 | RK78 | DP78
      INTEGRATOR_FIXED_STEPSIZE  60.0  % (0, INF) s
      % INTEGRATOR_INITIAL_STEPSIZE  60.0  % (0, INF) s
      % INTEGRATOR_ERROR_TOLERANCE  1.0E-12  % (0, INF)


      % OUTPUT SETTINGS

      OUTPUT_DIRECTORY_PATH  ''
      OUTPUT_FILE_NAME  '$FILENAME'

      OUTPUT_NUMERIC_PRECISION  8  % [1, INF)

      OUTPUT_INPUT_SETTINGS  YES  % NO | YES
      OUTPUT_RESULTS_COLUMNS_DESCRIPTIONS  YES  % NO | YES
      OUTPUT_PROPAGATION_TIME  YES  % NO | YES
      OUTPUT_COMPUTATION_TIME  YES  % NO | YES
      OUTPUT_PROPAGATION_TERMINATION_CAUSE  YES  % NO | YES

      OUTPUT_BODY_KEPLERIAN_STATE  YES  % NO | YES
      OUTPUT_BODY_CARTESIAN_STATE  NO  % NO | YES
      OUTPUT_SUN_POSITION  NO  % NO | YES
      OUTPUT_MOON_POSITION  NO  % NO | YES

      OUTPUT_ONE_IN_EVERY_INTEGRATION_STEPS  1  % [1, INF)
      OUTPUT_ONLY_LAST_INTEGRATION_STEP  NO  % NO | YES

      PRINT_SUCCESS_MESSAGE_AFTER_PROPAGATION  YES  % NO | YES
      DELETE_INPUT_FILE_AFTER_PROPAGATION  NO  % NO | YES


      % DSST PROPAGATOR SPECIFIC OUTPUT SETTINGS

      OUTPUT_DSST_MEAN_ELEMENT_RATES  NO  % NO | YES
      OUTPUT_DSST_MEAN_ELEMENT_RATES_ZONAL_TERMS  NO  % NO | YES
      OUTPUT_DSST_MEAN_ELEMENT_RATES_SUN_GRAVITY  NO  % NO | YES
      OUTPUT_DSST_MEAN_ELEMENT_RATES_MOON_GRAVITY  NO  % NO | YES
      OUTPUT_DSST_MEAN_ELEMENT_RATES_ATMOSPHERIC_DRAG  NO  % NO | YES
      OUTPUT_DSST_MEAN_ELEMENT_RATES_SOLAR_RADIATION_PRESSURE  NO  % NO | YES

      OUTPUT_DSST_SHORT_PERIOD_TERMS  NO  % NO | YES
      OUTPUT_DSST_SHORT_PERIOD_TERMS_ZONAL_TERMS  NO  % NO | YES
      OUTPUT_DSST_SHORT_PERIOD_TERMS_SUN_GRAVITY  NO  % NO | YES
      OUTPUT_DSST_SHORT_PERIOD_TERMS_MOON_GRAVITY  NO  % NO | YES
      OUTPUT_DSST_SHORT_PERIOD_TERMS_ATMOSPHERIC_DRAG  NO  % NO | YES
      OUTPUT_DSST_SHORT_PERIOD_TERMS_SOLAR_RADIATION_PRESSURE  NO  % NO | YES

      OUTPUT_DSST_COMPUTATION_TIMES  NO  % NO | YES
      OUTPUT_DSST_COMPUTATION_TIMES_ZONAL_TERMS  NO  % NO | YES
      OUTPUT_DSST_COMPUTATION_TIMES_SUN_GRAVITY  NO  % NO | YES
      OUTPUT_DSST_COMPUTATION_TIMES_MOON_GRAVITY  NO  % NO | YES
      OUTPUT_DSST_COMPUTATION_TIMES_ATMOSPHERIC_DRAG  NO  % NO | YES
      OUTPUT_DSST_COMPUTATION_TIMES_SOLAR_RADIATION_PRESSURE  NO  % NO | YES


      % PREVIOUS OUTPUT FILES

      SKIP_PROPAGATION_IF_OUTPUT_FILE_EXISTS  NO  % NO | YES

      SKIP_PROPAGATION_IF_RESULTS_FOUND  YES  % NO | YES
      ALLOW_RESUMING_PROPAGATION_FROM_RESULTS  YES  % NO | YES

      SEARCH_OUTPUT_DIRECTORY_PATH  ''
      SEARCH_OUTPUT_FILENAME_PATTERN  '.+'
      SEARCH_IN_SUBDIRECTORIES  NO  % NO | YES
      SEARCH_IGNORE_FILE_CONTENTS  NO  % NO | YES

      OUTPUT_FOR_SKIPPED_PROPAGATION  DUPLICATE  % NONE | REFERENCE | DUPLICATE


      % WARNINGS AND ERRORS

      NON_DEFINED_VARIABLE  USE_DEFAULT  % USE_DEFAULT | WARN | ERROR
      UNRECOGNIZED_VARIABLE_NAME  WARN  % IGNORE | WARN | ERROR
      NON_VALID_VALUE  ERROR  % USE_DEFAULT | WARN | ERROR

      MULTIPLY_DEFINED_VARIABLE_DIF_FILES  USE_LAST  % USE_LAST | WARN | ERROR
      MULTIPLY_DEFINED_VARIABLE_SAME_FILE  WARN  % USE_LAST | WARN | ERROR

      SKIPPING_PROPAGATION  WARN  % SILENT | WARN
      RESUMING_PROPAGATION_FROM_OUTPUT  WARN  % SILENT | WARN

    '''
    "description": "Prints a list of all the recognized parameters with the default values and the allowed (range of) values."


# tespvar snippets
".source.tespvar":

  "Attach statement":
    "prefix": "attach"
    "body": "#ATTACH ${1:VARIABLE1} ${2:VARIABLE2 VARIABLE3 ...}"
    "description": "Specify that a set of variables (separated by spaces) must be used in a one-to-one basis to generate the input files. For instance, when this statement is not used, if VARIABLE1 has 5 values and VARIABLE2 has 3 values, a total of 5*3 = 15 cases are generated. However, when this statement is used for variables that have the same name of values, e.g. 4 values for VARIABLES1 and also 4 values for VARIABLES2, then these values are matched one-to-one and only 4 input files (intead of 4*4 = 16) are generated."

  "linspace":
    "prefix": "linspace"
    "body": "linspace(${1:ini},${2:end},${3:N})"
    "description": "Generate linearly spaced vector."
    "descriptionMoreURL": "https://mathworks.com/help/matlab/ref/linspace.html"

  "sprintf":
    "prefix": "sprintf"
    "body": "sprintf('${1:formatSpec}',${2:A1,...,An})"
    "description": "Format data into string."
    "descriptionMoreURL": "https://mathworks.com/help/matlab/ref/sprintf.html"

