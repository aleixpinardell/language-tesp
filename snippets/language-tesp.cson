# Common tespin+tespvar variables
".source.tespin, .source.tespvar":

  "Geopotential model":
    "prefix": "GEOPOTENTIAL_MODEL"
    "leftLabel": "enum"
    "body": "GEOPOTENTIAL_MODEL  ${1:GGM02C}${2:  % GGM02C | GGM02S${3:\n}}"
    "description": "The model used to describe the Earth's gravity field. Possible values: GGM02C, gravity model based on data from GRACE and other measurements; GGM02S, gravity model based exclusively on data from GRACE."
    "descriptionMoreURL": "http://www.csr.utexas.edu/grace/gravity/ggm02/"

  "Degree of the geopotential":
    "prefix": "GEOPOTENTIAL_DEGREE"
    "leftLabel": "int"
    "body": "GEOPOTENTIAL_DEGREE  ${1:2}${2:  % [1, 160]${3:\n}}"
    "description": "The degree of the spherical harmonics expansion of the geopotential."

  "Order of the geopotential":
    "prefix": "GEOPOTENTIAL_ORDER"
    "leftLabel": "int"
    "body": "GEOPOTENTIAL_ORDER  ${1:0}${2:  % [0, 160]${3:\n}}"
    "description": "The order of the spherical harmonics expansion of the geopotential."

  "Sun's gravity":
    "prefix": "THIRD_BODY_ATTRACTION_SUN"
    "leftLabel": "bool"
    "body": "THIRD_BODY_ATTRACTION_SUN  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether the Sun's thrid-body central gravity should be included in the acceleration model."

  "Moon's gravity":
    "prefix": "THIRD_BODY_ATTRACTION_MOON"
    "leftLabel": "bool"
    "body": "THIRD_BODY_ATTRACTION_MOON  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether the Moon's thrid-body central gravity should be included in the acceleration model."

  "Atmospheric drag":
    "prefix": "ATMOSPHERIC_DRAG"
    "leftLabel": "bool"
    "body": "ATMOSPHERIC_DRAG  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether the drag caused by Earth's atmosphere should be included in the acceleration model."

  "Atmospheric model":
    "prefix": "ATMOSPHERIC_MODEL"
    "leftLabel": "enum"
    "body": "ATMOSPHERIC_MODEL  ${1:NRLMSISE00}${2:  % NRLMSISE00 | EXPONENTIAL${3:\n}}"
    "description": "Model used for the determination of atmospheric density."

  "Space weather data file":
    "prefix": "SPACE_WEATHER_FILE_PATH"
    "leftLabel": "std::string"
    "body": "SPACE_WEATHER_FILE_PATH  ${1:''}\n"
    "description": "Relative path to the text file containing space weather data for the NRLMSISE00 atmosphere model. The file should be formatted as in https://celestrak.com/SpaceData/sw19571001.txt. When not specified or when assigned to an empty string, the default file contained in Tudat is used. This file was downloaded using the previous link but may not be up to date."
    "descriptionMoreURL": "https://celestrak.com/SpaceData/"

  "Solar radiation pressure":
    "prefix": "SOLAR_RADIATION_PRESSURE"
    "leftLabel": "bool"
    "body": "SOLAR_RADIATION_PRESSURE  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether the force caused by the solar flux should be included in the acceleration model."

  "Ignore Earth's shadow":
    "prefix": "IGNORE_ECLIPSES"
    "leftLabel": "bool"
    "body": "IGNORE_ECLIPSES  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the Earth's shadow should be ignored during the assessment of solar radiation pressure."

  "Body mass":
    "prefix": "BODY_MASS"
    "leftLabel": "double"
    "body": "BODY_MASS  ${1:100.0}${2:  % (0, INF) kg${3:\n}}"
    "description": "The mass of the propagated body, in kilograms."

  "Body cross-sectional area":
    "prefix": "BODY_CROSS_SECTIONAL_AREA"
    "leftLabel": "double"
    "body": "BODY_CROSS_SECTIONAL_AREA  ${1:10.0}${2:  % (0, INF) m\^2${3:\n}}"
    "description": "The cross-sectional area of the propagated body, in squared meters."

  "Altitude dependent drag coefficient":
    "prefix": "ALTITUDE_DEPENDENT_DRAG_COEFFICIENT"
    "leftLabel": "bool"
    "body": "ALTITUDE_DEPENDENT_DRAG_COEFFICIENT  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the drag coefficient should be determined automaically depending on the altitude of the propagated body, which is assumed to be a tumbling plate. The values range from 2 to 3. If set to YES, the value of BODY_DRAG_COEFFICIENT is ignored."

  "Body drag coefficient":
    "prefix": "BODY_DRAG_COEFFICIENT"
    "leftLabel": "double"
    "body": "BODY_DRAG_COEFFICIENT  ${1:2.5}${2:  % (0, INF)${3:\n}}"
    "description": "The drag coefficient of the propagated body. Ignored if ALTITUDE_DEPENDENT_DRAG_COEFFICIENT is set to YES."

  "Body radiation pressure coefficient":
    "prefix": "BODY_RADIATION_PRESSURE_COEFFICIENT"
    "leftLabel": "double"
    "body": "BODY_RADIATION_PRESSURE_COEFFICIENT  ${1:1.5}${2:  % [1, 2]${3:\n}}"
    "description": "The radiation pressure coefficient of the propagated body."

  "Initial epoch":
    "prefix": "INITIAL_EPOCH"
    "leftLabel": "double"
    "body": "INITIAL_EPOCH  ${1:0.0}${2:  % (-INF, INF) seconds since J2000, or formatted date: 'YYYY-MM-DD hh:mm:ss'${3:\n}}"
    "description": "The initial epoch of the propagation, in seconds since J2000 or using the date formatt 'YYYY-MM-DD hh:mm:ss'. MM and DD can have one or two digits. hh ranges from 0 to 23. ss can be provided with decimal figures. The time-part can be partially or totally ommited. When doing so, it is assumed to be 00:00:00."

  "Initial semi-major axis":
    "prefix": "INITIAL_SEMIMAJOR_AXIS"
    "leftLabel": "double"
    "body": "INITIAL_SEMIMAJOR_AXIS  ${1:7371.0}${2:  % (6371, INF) km${3:\n}}"
    "description": "The initial semi-major axis of the orbit, in km. Has to be used together with INITIAL_ECCENTRICITY. Cannot be used if INITIAL_PERIGEE_ALTITUDE and INITIAL_APOGEE_ALTITUDE are provided."

  "Initial eccentricity":
    "prefix": "INITIAL_ECCENTRICITY"
    "leftLabel": "double"
    "body": "INITIAL_ECCENTRICITY  ${1:0.0}${2:  % [0, 1)${3:\n}}"
    "description": "The initial eccentricity of the orbit. The propagation of parabollic or hyperbolic trajectories is not supported. Has to be used together with INITIAL_SEMIMAJOR_AXIS. Cannot be used if INITIAL_PERIGEE_ALTITUDE and INITIAL_APOGEE_ALTITUDE are provided."

  "Initial inclination":
    "prefix": "INITIAL_INCLINATION"
    "leftLabel": "double"
    "body": "INITIAL_INCLINATION  ${1:0.0}${2:  % [0, 180] deg${3:\n}}"
    "description": "The initial inclination of the orbit, in degrees."

  "Initial argument of perigee":
    "prefix": "INITIAL_ARGUMENT_PERIGEE"
    "leftLabel": "double"
    "body": "INITIAL_ARGUMENT_PERIGEE  ${1:0.0}${2:  % [0, 360] deg${3:\n}}"
    "description": "The initial argument of perigee of the orbit, in degrees."

  "Initial longitude of the ascending node":
    "prefix": "INITIAL_LONGITUDE_ASCENDING_NODE"
    "leftLabel": "double"
    "body": "INITIAL_LONGITUDE_ASCENDING_NODE  ${1:0.0}${2:  % [0, 360] deg${3:\n}}"
    "description": "The initial longitude of the ascending node of the orbit, in degrees."

  "Initial true anomaly":
    "prefix": "INITIAL_TRUE_ANOMALY"
    "leftLabel": "double"
    "body": "INITIAL_TRUE_ANOMALY  ${1:0.0}${2:  % [0, 360] deg${3:\n}}"
    "description": "The initial true anomaly of the propagated body, in degrees."

  "Initial perigee altitude":
    "prefix": "INITIAL_PERIGEE_ALTITUDE"
    "leftLabel": "double"
    "body": "INITIAL_PERIGEE_ALTITUDE  ${1:1000.0}${2:  % (0, INF) km${3:\n}}"
    "description": "The initial perigee altitude of the orbit, in kilometres. Has to be used together with INITIAL_APOGEE_ALTITUDE. Cannot be used if INITIAL_SEMIMAJOR_AXIS and INITIAL_ECCENTRICITY are provided."

  "Initial apogee altitude":
    "prefix": "INITIAL_APOGEE_ALTITUDE"
    "leftLabel": "double"
    "body": "INITIAL_APOGEE_ALTITUDE  ${1:1000.0}${2:  % (0, INF) km${3:\n}}"
    "description": "The initial apogee altitude of the orbit, in kilometres. Has to be used together with INITIAL_PERIGEE_ALTITUDE. Cannot be used if INITIAL_SEMIMAJOR_AXIS and INITIAL_ECCENTRICITY are provided."

  "Preload celestial bodies data":
    "prefix": "PRELOAD_CELESTIAL_BODIES_DATA"
    "leftLabel": "bool"
    "body": "PRELOAD_CELESTIAL_BODIES_DATA  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to preload the ephemeris and rotational model data from Spice for the Earth, the Sun and the Moon before the propagation for the period defined by INITIAL_EPOCH and END_EPOCH or PROPAGATION_PERIOD, or to access Spice at each integration step. In general, preloading the celestial bodies data is recommended, as it can speed up the propagation significantly. However, for long propagations (several decades), this will require using large amounts of memory (several gigabytes). When the body is expected to have a high chance of re-entering at an epoch significantly sooner than the one determined by END_EPOCH or by INITIAL_EPOCH + PROPAGATION_PERIOD, then setting PRELOAD_CELESTIAL_BODIES_DATA to NO may be benefitial in terms of both memory useage and CPU performance."

  "End epoch":
    "prefix": "END_EPOCH"
    "leftLabel": "double"
    "body": "END_EPOCH  ${1:2015/12/31}${2:  % (-INF, INF) seconds since J2000, or formatted date: 'YYYY-MM-DD hh:mm:ss'${3:\n}}"
    "description": "The epoch at which the propagation will be stopped, if it has not finished yet. As for INITIAL_EPOCH, the value can be provided in seconds since J2000 or using the date format 'YYYY-MM-DD hh:mm:ss'. MM and DD can have one or two digits. hh ranges from 0 to 23. ss can be provided with decimal figures. The time-part can be partially or totally ommited. When doing so, it is assumed to be 00:00:00. The most restrictive of END_EPOCH, PROPAGATION_PERIOD and REENTRY_ALTITUDE will determine the actual duration of the propagation."

  "Maximum propagation period":
    "prefix": "PROPAGATION_PERIOD"
    "leftLabel": "double"
    "body": "PROPAGATION_PERIOD  ${1:3.1558e8}${2:  % (0, INF) seconds${3:\n}}"
    "description": "The period, in seconds, after which the propagation will be stopped, if it has not finished yet. The most restrictive of END_EPOCH, PROPAGATION_PERIOD and REENTRY_ALTITUDE will determine the actual duration of the propagation."

  "Re-entry altitude":
    "prefix": "REENTRY_ALTITUDE"
    "leftLabel": "double"
    "body": "REENTRY_ALTITUDE  ${1:0.0}${2:  % [0, INF) km${3:\n}}"
    "description": "The re-entry altitude, in kilometres. If the propagated body's altitude gets below this value, the propagation will be terminated. If the re-entry altitude is chosen too low, the propagation may actually terminate before reaching the specified limit. If the re-entry altitude is set to 0 (or not specified), the re-entry condition will not be evaluated, which will increase performance. The most restrictive of END_EPOCH, PROPAGATION_PERIOD and REENTRY_ALTITUDE will determine the actual duration of the propagation."

  "Propagator name":
    "prefix": "PROPAGATOR_NAME"
    "leftLabel": "enum"
    "body": "PROPAGATOR_NAME  ${1:COWELL}${2:  % COWELL | ENCKE${3:\n}}"
    "description": "The propagator to be used. Possible values: COWELL, the state of the body is propagated directly; ENCKE, the difference between the state of the body and a reference, Keplerian orbit is propagated. The Cowell propagator is more efficient for highly-perturbed orbits, while the Encke propagator can provide better performance for slightly-perturbed orbits."

  "Integrator name":
    "prefix": "INTEGRATOR_NAME"
    "leftLabel": "enum"
    "body": "INTEGRATOR_NAME  ${1:RK4}${2:  % RK4 | RK45 | RK56 | RK78 | DP78${3:\n}}"
    "description": "The integrator to be used. Possible values: RK4, fixed-stepsize integrator based on the Runge-Kutta 4 method; RK45, variable-stepsize integrator based on the Runge-Kutta-Fehlberg 4(5) method; RK56, variable-stepsize integrator based on the Runge-Kutta-Fehlberg 5(6) method; RK78, variable-stepsize integrator based on the Runge-Kutta-Fehlberg 7(8) method; DP78, variable-stepsize integrator based on the Dormand-Prince 7(8) method."

  "Stepsize of the fixed-stepsize integrator":
    "prefix": "INTEGRATOR_FIXED_STEPSIZE"
    "leftLabel": "double"
    "body": "INTEGRATOR_FIXED_STEPSIZE  ${1:60.0}${2:  % (0, INF) s${3:\n}}"
    "description": "The stepsize of the integrator, in seconds. Only applicable if a fixed-stepsize integrator is being used."

  "Initial stepsize of the variable-stepsize integrator":
    "prefix": "INTEGRATOR_INITIAL_STEPSIZE"
    "leftLabel": "double"
    "body": "INTEGRATOR_INITIAL_STEPSIZE  ${1:60.0}${2:  % (0, INF) s${3:\n}}"
    "description": "The initial stepsize of the integrator, in seconds. Only applicable if a variable-stepsize integrator is being used. The first step is performed with the specified stepsize, while the following stepsizes are determined so that the values of the relative or absolute errors do not exceed INTEGRATOR_ERROR_TOLERANCE."

  "Error tolerance of the variable-stepsize integrator":
    "prefix": "INTEGRATOR_ERROR_TOLERANCE"
    "leftLabel": "double"
    "body": "INTEGRATOR_ERROR_TOLERANCE  ${1:1.0E-12}${2:  % (0, INF)${3:\n}}"
    "description": "The error tolerance of the variable-stepsize integrator. Ignored if a fixed-stepsize integrator is being used. The first step is performed with the stepsize defined by INTEGRATOR_INITIAL_STEPSIZE, while the following stepsizes are determined so that the values of the relative or absolute errors do not exceed INTEGRATOR_ERROR_TOLERANCE."

  "Altitude limit for the consideration of drag by the DSST propagator":
    "prefix": "DSST_ALTITUDE_LIMIT_ATMOSPHERIC_DRAG"
    "leftLabel": "double"
    "body": "DSST_ALTITUDE_LIMIT_ATMOSPHERIC_DRAG  ${1:600}${2:  % [0, INF) km${3:\n}}"
    "description": "The altitude, in km, above which drag is neglected for the computation of the effects of atmospheric drag on the mean element rates. Is set to 0, drag is never neglected and thus considered throught the entire orbit."

  "Number of nodes for the Gaussian quadrature of atmospheric drag":
    "prefix": "DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG"
    "leftLabel": "int"
    "body": "DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG  ${1:40}${2:  % [1, 64]${3:\n}}"
    "description": "The number of nodes to be used for solving the averaging integral of atmospheric drag. If the to 2 or larger, a Gaussian quadrature is performed to solve the integral. When DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG_IS_SCALABLE is set to YES, the actual number of nodes is determined based on the value of DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG and the percentage of the orbit in which drag cannot be neglected (determined based on the value of DSST_ALTITUDE_LIMIT_ATMOSPHERIC_DRAG), and an odd number is always chosen so that the central node (perigee) is included. When DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG is set to 1, drag is only evaluated at perigee and a semi-empirical relationship is then used to estimate the total drag throught the entire orbit."

  "Scalability of the number of nodes for the Gaussian quadrature of atmospheric drag":
    "prefix": "DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG_IS_SCALABLE"
    "leftLabel": "bool"
    "body": "DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG_IS_SCALABLE  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether the number of nodes used for solving the averaging integral of atmospheric drag is scalable. When DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG_IS_SCALABLE is set to YES, the actual number of nodes is determined based on the value of DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG and the percentage of the orbit in which drag cannot be neglected (determined based on the value of DSST_ALTITUDE_LIMIT_ATMOSPHERIC_DRAG), and an odd number is always chosen so that the central node (perigee) is included. When DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG_IS_SCALABLE is set to NO, the value of DSST_QUADRATURE_NODES_ATMOSPHERIC_DRAG is directly used, even if it is an even number."

  "Number of nodes for the Gaussian quadrature of solar radiation pressure":
    "prefix": "DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE"
    "leftLabel": "int"
    "body": "DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE  ${1:10}${2:  % [2, 64]${3:\n}}"
    "description": "The number of nodes to be used for solving the averaging integral of solar radiation pressure when eclipses are considered. A Gaussian quadrature is performed to solve the integral. When DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE_IS_SCALABLE is set to YES, the actual number of nodes is determined based on the value of DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE and the percentage of the orbit during which the satellite does not experience eclipse."

  "Scalability of the number of nodes for the Gaussian quadrature of solar radiation pressure":
    "prefix": "DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE_IS_SCALABLE"
    "leftLabel": "bool"
    "body": "DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE_IS_SCALABLE  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether the number of nodes used for solving the averaging integral of solar radiation pressure is scalable. When DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE_IS_SCALABLE is set to YES, the actual number of nodes is determined based on the value of DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE and the percentage of the orbit during which the satellite does not experience eclipse. When DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE_IS_SCALABLE is set to NO, the value of DSST_QUADRATURE_NODES_SOLAR_RADIATION_PRESSURE is directly used, regardless of the duration of the eclipse."

  "Maximum value for the first index of the serie expansion for Sun gravity":
    "prefix": "DSST_THIRD_BODY_ATTRACTION_SUN_S"
    "leftLabel": "int"
    "body": "DSST_THIRD_BODY_ATTRACTION_SUN_S  ${1:2}${2:  % [2, INF)${3:\n}}"
    "description": "The maximum value for the first index of the serie expansion performed to compute the effect of the Sun's thrid-body central gravity on the mean element rates."

  "Maximum value for the second index of the serie expansion for Sun gravity":
    "prefix": "DSST_THIRD_BODY_ATTRACTION_SUN_N"
    "leftLabel": "int"
    "body": "DSST_THIRD_BODY_ATTRACTION_SUN_N  ${1:2}${2:  % [2, INF)${3:\n}}"
    "description": "The maximum value for the second index of the serie expansion performed to compute the effect of the Sun's thrid-body central gravity on the mean element rates."

  "Maximum value for the first index of the serie expansion for Moon gravity":
    "prefix": "DSST_THIRD_BODY_ATTRACTION_MOON_S"
    "leftLabel": "int"
    "body": "DSST_THIRD_BODY_ATTRACTION_MOON_S  ${1:2}${2:  % [2, INF)${3:\n}}"
    "description": "The maximum value for the first index of the serie expansion performed to compute the effect of the Moon's thrid-body central gravity on the mean element rates."

  "Maximum value for the second index of the serie expansion for Moon gravity":
    "prefix": "DSST_THIRD_BODY_ATTRACTION_MOON_N"
    "leftLabel": "int"
    "body": "DSST_THIRD_BODY_ATTRACTION_MOON_N  ${1:2}${2:  % [2, INF)${3:\n}}"
    "description": "The maximum value for the second index of the serie expansion performed to compute the effect of the Moon's thrid-body central gravity on the mean element rates."

  "Maximum value for the first index of the serie expansion for solar radiation pressure":
    "prefix": "DSST_SOLAR_RADIATION_PRESSURE_S"
    "leftLabel": "int"
    "body": "DSST_SOLAR_RADIATION_PRESSURE_S  ${1:2}${2:  % [2, INF)${3:\n}}"
    "description": "The maximum value for the first index of the serie expansion performed to compute the effect of solar radiation pressure on the mean element rates. Ignored when eclipses are considered."

  "Maximum value for the second index of the serie expansion for solar radiation pressure":
    "prefix": "DSST_SOLAR_RADIATION_PRESSURE_N"
    "leftLabel": "int"
    "body": "DSST_SOLAR_RADIATION_PRESSURE_N  ${1:2}${2:  % [2, INF)${3:\n}}"
    "description": "The maximum value for the second index of the serie expansion performed to compute the effect of solar radiation pressure on the mean element rates. Ignored when eclipses are considered."

  "Output directory path":
    "prefix": "OUTPUT_DIRECTORY_PATH"
    "leftLabel": "std::string"
    "body": "OUTPUT_DIRECTORY_PATH  '$1'${2:\n}"
    "description": "Path to the directory where the output file will be generated. Relative or absolute paths can be used. Relative paths are defined with respect to the directory in which the file containing this statement is located."

  "Output file's name":
    "prefix": "OUTPUT_FILE_NAME"
    "leftLabel": "std::string"
    "body": "OUTPUT_FILE_NAME  '${1:$FILENAME}'${2:\n}"
    "description": "The filename of the output file. It is not necessary to indicate the \"tespout\" extension. $FILENAME will be replaced by the original input file's name during runtime."

  "Numeric precision":
    "prefix": "OUTPUT_NUMERIC_PRECISION"
    "leftLabel": "int"
    "body": "OUTPUT_NUMERIC_PRECISION  ${1:8}${2:  % [1, INF)${3:\n}}"
    "description": "The number of significant digits to be used when printing the results to the output file."

  "Output input settings":
    "prefix": "OUTPUT_INPUT_SETTINGS"
    "leftLabel": "bool"
    "body": "OUTPUT_INPUT_SETTINGS  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include the settings for the propagation in the output file. These do not only include the variables explicitily specified in the input file(s) but also the variables that took the default values. Some of the variables, which are not relevant for the results of the propagation, are not included."

  "Output description of the columns of the result matrix":
    "prefix": "OUTPUT_RESULTS_COLUMNS_DESCRIPTIONS"
    "leftLabel": "bool"
    "body": "OUTPUT_RESULTS_COLUMNS_DESCRIPTIONS  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include a description of the columns of the result matrix in the output file. This description indicates which physical property each of the columns corresponds to, and the corresponding units."

  "Output propagation time":
    "prefix": "OUTPUT_PROPAGATION_TIME"
    "leftLabel": "bool"
    "body": "OUTPUT_PROPAGATION_TIME  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include the computation time that was required to complete the propagation, in seconds, in the output file. This only includes integrating the equations of motion from the initial to the final epoch. Does not include model set-up or results export."

  "Output computation time":
    "prefix": "OUTPUT_COMPUTATION_TIME"
    "leftLabel": "bool"
    "body": "OUTPUT_COMPUTATION_TIME  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include the total computation time that was required to complete the propagation, in seconds, in the output file. This includes every task except exporting results."

  "Output propagation termination cause":
    "prefix": "OUTPUT_PROPAGATION_TERMINATION_CAUSE"
    "leftLabel": "bool"
    "body": "OUTPUT_PROPAGATION_TERMINATION_CAUSE  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include the cause of termination of the propagation in the output file. The possible outputted values are END_EPOCH_REACHED, MAXIMUM_PROPAGATION_PERIOD_REACHED, REENTRY_ALTITUDE_REACHED, PROPAGATION_ERROR_THROWN and UNKNOWN."

  "Output body Keplerian state":
    "prefix": "OUTPUT_BODY_KEPLERIAN_STATE"
    "leftLabel": "bool"
    "body": "OUTPUT_BODY_KEPLERIAN_STATE  ${1:YES}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the Keplerian state of the propagated body at the requested integration steps. The columns, from left to right, correspond to these magnitudes (units): semi-major axis (m), eccentricty, inclination (rad), argument of perigee (rad), longitude of the ascending node (rad) and true anomaly (rad)."

  "utput body Cartesian state":
    "prefix": "OUTPUT_BODY_CARTESIAN_STATE"
    "leftLabel": "bool"
    "body": "OUTPUT_BODY_CARTESIAN_STATE  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the Cartesian state of the propagated body, defined in an intertial J2000-Solar-System-barycentre-oriented, Earth-centred reference frame, at the requested integration steps. The columns, from left to right, correspond to these magnitudes (units): position X axis (m), position Y axis (m), position Z axis (m), velocity X axis (m/s), velocity Y axis (m/s), velocity Z axis (m/s)."

  "Output Sun position":
    "prefix": "OUTPUT_SUN_POSITION"
    "leftLabel": "bool"
    "body": "OUTPUT_SUN_POSITION  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include three columns in the results matrix containing the position of the Sun, defined in an intertial J2000-Solar-System-barycentre-oriented, Earth-centred reference frame, at the requested integration steps. The columns, from left to right, correspond to these magnitudes (units): position X axis (m), position Y axis (m), position Z axis (m)."

  "Output Moon position":
    "prefix": "OUTPUT_MOON_POSITION"
    "leftLabel": "bool"
    "body": "OUTPUT_MOON_POSITION  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include three columns in the results matrix containing the position of the Moon, defined in an intertial J2000-Solar-System-barycentre-oriented, Earth-centred reference frame, at the requested integration steps. The columns, from left to right, correspond to these magnitudes (units): position X axis (m), position Y axis (m), position Z axis (m)."

  "Output total mean element rates":
    "prefix": "OUTPUT_DSST_MEAN_ELEMENT_RATES"
    "leftLabel": "bool"
    "body": "OUTPUT_DSST_MEAN_ELEMENT_RATES  ${1:NO}${2:  % NO | YES${3:\n}}"
    "description": "Whether to include six columns in the results matrix containing the mean element rates due to all the included disturbing forces."

  "Output mean element rates due to zonal terms":
    "prefix": "OUTPUT_DSST_MEAN_ELEMENT_RATES_ZONAL_TERMS"
